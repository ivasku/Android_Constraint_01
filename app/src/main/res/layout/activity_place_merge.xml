<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".PlaceMerge">

    <ImageView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/images"
            android:scaleType="centerCrop"
            tools:layout_editor_absoluteY="268dp" tools:layout_editor_absoluteX="76dp" android:id="@+id/image"/>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/button"
            android:src="@drawable/ic_directions"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    <include layout="@layout/header_template" />
</androidx.constraintlayout.widget.ConstraintLayout>